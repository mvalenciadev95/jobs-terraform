# Processing Service - Environment Variables

# Server Configuration
PORT=3002

# AWS Configuration
AWS_REGION=us-east-1

# S3 Configuration (MinIO for local development)
USE_LOCALSTACK=true
S3_BUCKET_NAME=twl-raw-data
S3_ENDPOINT=http://localhost:9000

# SQS Configuration (LocalStack for local development)
SQS_QUEUE_URL=http://localhost:4566/000000000000/twl-processing-queue
SQS_ENDPOINT=http://localhost:4566

# MongoDB Configuration
MONGODB_URI=mongodb://admin:admin@localhost:27017/twl_pipeline?authSource=admin

# Processing Configuration
MAX_CONCURRENCY=5
